{
  "manifest_version": 2,
  "name": "OCSessions",
  "version": "1.0",
  "description": "Extension pour consulter tout l'historique de vos comptes-rendus.",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://openclassrooms.com/fr/dashboard/*"
      ],
      "js": [
        "src/main.js",
        "src/buildFront.js"
      ]
    }
  ]
}